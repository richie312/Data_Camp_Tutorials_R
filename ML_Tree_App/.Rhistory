get_dataset<- function(data_x, split_ratio = 0.8, set = 'train'){
if (set == 'train'){
n <- nrow(data_x)
shuffled <- data_x[sample(n),]
# Split the data in train and test
train <- shuffled[1:round(split_ratio * n),]
test <- shuffled[(round(split_ratio * n) + 1):n,]
return(train)
}
if(set == 'test'){
n <- nrow(data_x)
shuffled <- data_x[sample(n),]
# Split the data in train and test
train <- shuffled[1:round(split_ratio * n),]
test <- shuffled[(round(split_ratio * n) + 1):n,]
return(test)
}
else
return (NULL)
}
